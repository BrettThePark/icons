{
  "name": "@pxblue/icons",
  "version": "1.0.8",
  "description": "Px Blue icons",
  "main": "./iconfont/index.css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pxblue/icons.git"
  },
  "author": "Keller, Kraig <KraigKeller@eaton.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
  },
  "scripts": {
    "copy": "rm -r svg && mkdir svg && npx copyfiles -f design/**/**/*.svg svg",
    "build": "yarn copy && rm -r iconfont && mkdir iconfont && npx icon-font-generator svg/*.svg -o iconfont -n PXBlueIcons -p pxb",
    "sprites": "npx create-svg-sprite --optimize --input './svg' --output './svg' --name 'px_blue_icons' && npx create-svg-sprite --optimize --input './symbols' --output './symbols' --name 'symbols'"
  },
  "bugs": {
    "url": "https://github.com/pxblue/icons/issues"
  },
  "homepage": "https://github.com/pxblue/icons#readme",
  "devDependencies": {
    "copyfiles": "^2.1.0",
    "create-svg-sprite": "^1.0.4",
    "icon-font-generator": "^2.1.8"
  },
  "keywords": [
    "icons",
    "material",
    "pxblue"
  ]
}
